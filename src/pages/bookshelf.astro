---

// Component Imports
import DefaultLayout from "../layouts/DefaultLayout.astro";
import { AHBookCase } from "../web-components/ah-bookcase.js";

import { groupedBooks } from "src/utils/getBooksFromNotion";

const months = groupedBooks("month");
const years = groupedBooks("year");

const title = "bookshelf";
---

<DefaultLayout title={title}>
  <AHBookCase client:visible months={months} years={years}>DERP</AHBookCase>
</DefaultLayout>
